{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "color":{
            "defaultPalette": false,
            "palette": [
                {
                    "slug": "black",
                    "color": "#000000",
                    "name": "Black"
                },
                {
                    "slug": "white",
                    "color": "#ffffff",
                    "name": "White"
                }
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "tahoma",
                    "name": "Tahoma",
                    "slug": "tahoma"
                },
                {
                    "fontFamily": "arial",
                    "name": "Arial",
                    "slug": "arial"
                }
            ]
        }
    },
    "styles": {
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--tahoma)"
        }
    },
    "customTemplates": [
        {
            "name": "blank",
            "title": "Blank Template",
            "postTypes": [
                "page",
                "post"
            ]
        }
    ]
}